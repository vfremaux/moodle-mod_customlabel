{{!
    @template mod_customlabel/printscreen

    Classes required for JS:
    * no classes
}}
<!-- customlabeltype_coursedata/printscreen -->
{{> mod_customlabel/start }}

<div class="custombox-container {{labelclass}}">
    <script type="text/javascript">
        function customlabel_print_page() {
            window.print();
        }
    </script>

    <form>
        <input id="id_print_page" name="print" type="button" class="btn-primary" onclick="customlabel_print_page()" value="{{{print}}}">
    </form>
</div>
{{> mod_customlabel/end }}
<!-- /customlabeltype_coursedata/printscreen -->
