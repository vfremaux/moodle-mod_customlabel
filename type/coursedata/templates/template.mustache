                {{{teachingorganization}}}
            </td>
        </tr>
        {{/showteachingorganization}}

        {{#showprerequisites}}
        <tr class="custombox-{{labelclass}}">
            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}prerequisites, customlabeltype_coursedata{{/str}}
            </td>
            <td class="custombox-value {{labelclass}}" width="{{rightcolumnratio}}">
                {{{prerequisites}}}
            </td>
        </tr>
        {{/showprerequisites}}

        {{#showevaluation}}
        <tr class="custombox-{{labelclass}}">
            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}evaluation, customlabeltype_coursedata{{/str}}
            </td>
            <td class="custombox-value {{labelclass}}" width="{{rightcolumnratio}}">
                {{{evaluation}}}
            </td>
        </tr>
        {{/showevaluation}}

        {{#showoutcomes}}
