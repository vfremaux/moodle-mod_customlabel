            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}objectives, customlabeltype_coursedata{{/str}}
            </td>
            <td class="custombox-value {{labelclass}}" width="{{rightcolumnratio}}">
                {{{objectives}}}
            </td>
        </tr>
        {{/showobjectives}}

        {{#showconcepts}}
        <tr class="custombox-f{{labelclass}}">
            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}concepts, customlabeltype_coursedata{{/str}}
            </td>
            <td class="custombox-value {{labelclass}}" width="{{rightcolumnratio}}">
                {{{concepts}}}
            </td>
        </tr>
        {{/showconcepts}}

        {{#showduration}}
        <tr class="custombox-{{labelclass}}">
            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}duration, customlabeltype_coursedata{{/str}}
