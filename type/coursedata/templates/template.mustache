            </td>
        </tr>
        {{/showheadteacher}}
        
        {{#showlastupdate}}
        <tr class="custombox-{{labelclass}}">
            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}lastupdate, customlabeltype_coursedata{{/str}}
            </td>
            <td class="custombox-value {{labelclass}}" width="{{rightcolumnratio}}">
                {{{lastupdate}}}
            </td>
        </tr>
        {{/showlastupdate}}
    </table>
{{> mod_customlabel/end }}
<!-- /customlabeltype_coursedata/template -->
