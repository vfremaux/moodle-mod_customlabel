{{!
    @template mod_customlabel/courseclassifier

    Classes required for JS:
    * no classes
}}
<!-- customlabeltype_coursedata/template -->
{{> mod_customlabel/start }}
    <table width="100%">
        {{#tablecaption}}
        <tr class="custombox-{{labelclass}}">
            <td class="custombox-title {{labelclass}}" colspan="2">
                {{{tablecaption}}}
            </td>
        </tr>
        {{/tablecaption}}

        {{#showidnumber}}
        <tr class="custombox-{{labelclass}}">
            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}divainingid, customlabeltype_coursedata{{/str}}
            </td>
            <td class="custombox-value {{labelclass}}" width="{{rightcolumnratio}}">
                {{idnumber}}
            </td>
        </tr>
        {{/showidnumber}}

        {{#showtarget}}
        <tr class="custombox-{{labelclass}}">
            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}public, customlabeltype_coursedata{{/str}}
            </td>
            <td class="custombox-value {{labelclass}}" width="{{rightcolumnratio}}">
                {{{target}}}
            </td>
        </tr>
        {{/showtarget}}

        {{#showgoals}}
        <tr class="custombox-{{labelclass}}">
            <td class="custombox-param {{labelclass}}" width="{{leftcolumnratio}}">
                {{#str}}goals, customlabeltype_coursedata{{/str}}
            </td>
            <td class="custombox-value {{labelclass}}" width="{{rightcolumnratio}}">
                {{{goals}}}
            </td>
        </tr>
        {{/showgoals}}

        {{#showobjectives}}
