{{!
    @template mod_customlabel/seealso

    Classes required for JS:
    * mod/customlabel/customlabel

}}
<!-- customlabeltype_seealso/template -->
<table class="custombox-seealso" width="100%">

    <tr valign="top">

        <td class="custombox-header-thumb seealso" style="background-image : url({{{iconurl}}});" width="2%" rowspan="2">
        </td>

        <td class="custombox-header-caption seealso" width="98%">
            {{#str}}seealso, customlabeltype_seealso{{/str}}
        </td>
    </tr>

    <tr valign="top">
        <td class="custombox-content seealso">
            {{{seealso}}}
        </td>
    </tr>

</table>
<!-- customlabeltype_seealso/template -->
