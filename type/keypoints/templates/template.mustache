{{!
    @template mod_customlabel/keypoints

    Classes required for JS:
    * no classes

}}
<!-- customlabeltype_keypoints/template -->
<table class="custombox-keypoints" width="100%">

    <tr valign="top">
        <td class="custombox-header-thumb keypoints" width="2%" rowspan="2">
        </td>
        <td class="custombox-header-caption keypoints" width="98%">
            {{#str}}remind, customlabeltype_keypoints{{/str}}
        </td>
    </tr>

    <tr valign="top">
        <td class="custombox-content keypoints">
            <ul class="customlabel keypoints">
                {{#keypoints}}
                <li>
                    {{{keypoint}}}
                    {{#feedbackmark}}
                    <input type="checkbox" name="keypoint-{{item}}" > {{#str}}check, customlabeltype_keypoints{{/str}}
                    {{/feedbackmark}}
                </li>
                {{/keypoints}}
            </ul>
        </td>
    </tr>

    {{#teacherboard}}
    <tr>
        <td class="custombox-seealso-report">
        </td>
    </tr>
    {{/teacherboard}}

</table>
<!-- /customlabeltype_keypoints/template -->
